[core]
	editor = vim
[push]
	default = simple
[alias]
	logadog = log --all --decorate --oneline --graph
        co = checkout
        ci = commit
        st = status
        br = branch
        hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
        type = cat-file -t
        dump = cat-file -p

